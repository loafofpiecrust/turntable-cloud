{
  "name": "turntable-cloud",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "npm ci && tsc",
    "clean": "rm -rf lib/*",
    "bundle": "npm run clean && npm run build",
    "deploy": "npm run bundle && serverless deploy -f findSongOnYouTube",
    "deploy:prod": "npm run bundle && serverless deploy --stage prod"
  },
  "main": "index.js",
  "dependencies": {
    "fuzzball": "^1.2.0",
    "scrape-youtube": "0.0.4",
    "ytdl-core": "git://github.com/fent/node-ytdl-core.git#ab374c370a93770f749ff3817885eda81c570164"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.24",
    "@types/node": "^11.13.8",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "serverless": "^1.48.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "private": true
}
