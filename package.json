{
  "name": "turntable-cloud",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "clean": "rm -rf lib/*",
    "bundle": "npm run clean && npm run build",
    "deploy": "npm run bundle && serverless deploy -f findSongOnYouTube",
    "deploy:prod": "npm run bundle && serverless deploy --stage prod",
    "serve": "npm run build && serverless offline"
  },
  "main": "index.js",
  "dependencies": {
    "fuzzball": "^1.2.0",
    "scrape-youtube": "0.0.4",
    "ytdl-core": "0.29.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.24",
    "@types/node": "^11.13.8",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "serverless": "^1.48.1",
    "serverless-offline": "^5.8.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "lodash": ">=4.17.13"
  },
  "private": true
}
